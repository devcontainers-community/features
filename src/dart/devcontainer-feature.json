{
  "id": "dart",
  "name": "Dart",
  "description": "Installs the Dart SDK",
  "version": "1.0.0",
  "options": {
    "version": {
      "description": "Dart version",
      "type": "string",
      "default": "latest"
    }
  },
  "containerEnv": {
    "PATH": "/usr/lib/dart/bin:${PATH}"
  },
  "customizations": {
    "vscode": {
      "extensions": ["dart-code.dart-code"]
    }
  }
}
